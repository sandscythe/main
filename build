/snap/bin/zig build-exe a.zig -O ReleaseSmall

strip --strip-all a



offset=$(strings -t d a | grep -m1 'Linker: LLD' | awk '{print $1}')
if [ -n "$offset" ]; then
  printf '\0%.0s' {1..106} | dd of=a bs=1 seek="$offset" conv=notrunc
else
  echo "Linker string not found."
fi


strings a | grep "zig"
